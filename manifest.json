{
	  "name": "Moodle UTBM auto-connect",
	  "version": "0.1.3",

	  "permissions": [
	    "activeTab",
        "tabs",
        "https://moodle.utbm.fr/*",
        "https://cas.utbm.fr/*",
        "storage"
	  ],
	  "options_page": "options.html",
	  
	  "background": {
	    "scripts": ["autofill.js"],
	    "persistent": false
	  },
	  "browser_action" :
	  {
	    "default_icon" : "icon.png",
	    "default_title" : "Moodle utbm auto-connect"
	  },

      "icons" : {
	    "48" : "icon.png"
	  },

	  "manifest_version": 2
	}


